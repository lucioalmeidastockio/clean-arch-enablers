<settings>

    <servers>
        <server>
            <id>ossrh</id>
            <username>${SONATYPEUSERNAME}</username>
            <password>${SONATYPEPASS}</password>
        </server>
    </servers>

    <profiles>
        <profile>
            <id>ossrh</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <gpg.executable>gpg</gpg.executable>
                <gpg.passphrase>${GPGPASS}</gpg.passphrase>
            </properties>
        </profile>
    </profiles>

</settings>

